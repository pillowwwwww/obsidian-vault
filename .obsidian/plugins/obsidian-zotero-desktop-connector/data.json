{
  "database": "Zotero",
  "noteImportFolder": "02-Reading/mdnotes",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Format #1",
      "format": "formatted-bibliography",
      "cslStyle": "apa"
    }
  ],
  "exportFormats": [
    {
      "name": "my_import",
      "outputPathTemplate": "02-Reading/mdnotes/{{citekey}}.md",
      "imageOutputPathTemplate": "02-Reading/mdnotes/{{citekey}}/",
      "imageBaseNameTemplate": "image",
      "templatePath": "09-Templates/source-notes.md",
      "cslStyle": "chicago-note-bibliography"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": false,
  "whichNotesToOpenAfterImport": "first-imported-note"
}